    <artifactId>spring-extension-starter</artifactId>
    <packaging>pom</packaging>
    <properties>
        <spring-extension-starter-version>0.0.12</spring-extension-starter-version>
        <java.version>21</java.version>
    </properties>

    <modules>
        <module>spring-extension-starter-common</module>
